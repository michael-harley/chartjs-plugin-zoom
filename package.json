{
  "name": "chartjs-plugin-zoom",
  "description": "Plugin that enables zoom and pan functionality in Chart.js charts.",
  "version": "0.7.7",
  "license": "MIT",
  "jsdelivr": "dist/chartjs-plugin-zoom.min.js",
  "unpkg": "dist/chartjs-plugin-zoom.min.js",
  "main": "dist/chartjs-plugin-zoom.js",
  "module": "dist/chartjs-plugin-zoom.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/chartjs/chartjs-plugin-zoom.git"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "karma start --auto-watch --no-single-run --browsers chrome --grep",
    "lint": "eslint samples/**/*.html test/**/*.js src/**/*.js",
    "test": "concurrently \"npm:test-*\"",
    "test-lint": "npm run lint",
    "test-karma": "karma start --auto-watch --single-run --coverage --grep"
  },
  "files": [
    "dist/*.js"
  ],
  "devDependencies": {
    "chart.js": "^3.0.0-beta.7",
    "chartjs-test-utils": "^0.1.1",
    "concurrently": "^5.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.17.0",
    "eslint-config-chartjs": "^0.1.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-html": "^6.1.1",
    "jasmine": "^3.6.3",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-firefox-launcher": "^2.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "karma-rollup-preprocessor": "^7.0.5",
    "karma-spec-reporter": "0.0.32",
    "pixelmatch": "^5.2.1",
    "rollup": "^2.36.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-istanbul": "^3.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "peerDependencies": {
    "chart.js": "^3.0.0-beta.7"
  },
  "dependencies": {
    "hammerjs": "^2.0.8"
  }
}
